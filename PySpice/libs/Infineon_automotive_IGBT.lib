.PARAM AX0 = {NX3*LX2*1.4e-4}
.PARAM AX1 = {NX3*LX2*2.4e-4}
.PARAM AX2 = {NX3*LX2*2.58m}
.PARAM CX2 = {AX2*33.63n}
.PARAM AX3 = {NX3*LX2*8.6e-4}
.PARAM CX3 = {AX3*33.63n}
.PARAM AX4 = {NX3*LX2*1.0e-4}
.PARAM CX4 = {AX4*33.63n}
